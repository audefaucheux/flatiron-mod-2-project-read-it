<h1>Login</h1>
<%= form_tag(sessions_path) do %>
    <%= label_tag :email_address %>
    <%= text_field_tag :email_address %><br>
    <%= label_tag :password %>
    <%= password_field_tag :password %><br>
    <%= submit_tag :"Login" %>
<% end %>

<p>Don't have an account yet? <%= link_to "Sign up", new_user_path %></p>
  